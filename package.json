{
  "name": "vaulty",
  "displayName": "Vaulty",
  "description": "Simple and safe Ansible Vault plugin",
  "version": "0.1.2",
  "publisher": "codeflows",
  "license": "Apache-2.0",
  "homepage": "https://github.com/codeflows/vaulty/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/codeflows/vaulty/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codeflows/vaulty.git"
  },
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vaulty.decrypt"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "vaulty.decrypt",
        "title": "Vaulty: decrypt and view Ansible Vault file"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "lint": "tslint --project tsconfig.json -c tslint.json"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "prettier": "^1.4.4",
    "tslint": "^5.4.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  }
}